        <!--Questions-->
        <category>
            <pattern>ADDQUESTION *</pattern>
            <template>
                <think>
                    <set name="questions">
                        <get name="questions"/>
                        <star/>
                    </set>
                </think>
            </template>
        </category>

        <category>
            <pattern>GETQUESTIONS</pattern>
            <template>
                <get name="questions"/>
            </template>
        </category>

        <category>
            <pattern>ASKQUESTIONS * *</pattern>
            <template>
                <srai>ASKQUESTIONS
                    <star/>
                </srai>
                <srai>ASKQUESTIONS
                    <star index="2"/>
                </srai>
            </template>
        </category>
        <category>
            <pattern>ASKQUESTIONS *</pattern>
            <template>
            </template>
        </category>
        <category>
            <pattern>ASKQUESTIONS</pattern>
            <template></template>
        </category>

        <category>
            <pattern>ASKW * *</pattern>
            <template>
                <srai>ASKW
                    <star/>
                </srai>
                <srai>ASKW
                    <star index="2"/>
                </srai>
            </template>
        </category>
        <category>
            <pattern>ASKW *</pattern>
            <template></template>
        </category>
        <category>
            <pattern>ASKW</pattern>
            <template></template>
        </category>


